        "import numpy as np\n",
        "\n",
        "# Simulating some data based on the variable descriptions for demonstration purposes\n",
        "# Actual data would be needed for meaningful results\n",
        "\n",
        "# Simulate 'age' and 'total_intubation_time' data\n",
        "np.random.seed(0)\n",
        "simulated_data = pd.DataFrame({\n",
        "    'age': np.random.randint(20, 78, size=100),  # Age from 20 to 77\n",
        "    'total_intubation_time': np.random.randint(9, 101, size=100)  # Total intubation time from 9 to 100 seconds\n",
        "})\n",
        "\n",
        "# Calculate E[Y | X]\n",
        "E_Y_given_X = simulated_data.groupby('age')['total_intubation_time'].mean().reset_index(name='E[Y|X]')\n",
        "\n",
        "# Calculate V[Y | X]\n",
        "V_Y_given_X = simulated_data.groupby('age')['total_intubation_time'].var().reset_index(name='V[Y|X]')\n",
        "\n",
        "# Calculate E[X | Y > 30]\n",
        "E_X_given_Y_gt_30 = simulated_data[simulated_data['total_intubation_time'] > 30]['age'].mean()\n",
        "\n",
        "E_Y_given_X.head(), V_Y_given_X.head(), E_X_given_Y_gt_30\n"
